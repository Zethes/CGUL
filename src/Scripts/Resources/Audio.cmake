        ADD_LIBRARY(asound STATIC IMPORTED)
        SET_TARGET_PROPERTIES(asound PROPERTIES IMPORTED_LOCATION ${JATTA_DEPENDENCY_DIR}/lib/libasound.a)
        SET(JATTA_LIBRARIES ${JATTA_LIBRARIES} asound pthread rt)
    ENDIF()
